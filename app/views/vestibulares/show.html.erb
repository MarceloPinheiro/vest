<%- model_class = Vestibulare -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:descricao) %>:</strong></dt>
  <dd><%= @vestibulare.descricao %></dd>
  <dt><strong><%= model_class.human_attribute_name(:inicio) %>:</strong></dt>
  <dd><%= @vestibulare.inicio %></dd>
  <dt><strong><%= model_class.human_attribute_name(:fim) %>:</strong></dt>
  <dd><%= @vestibulare.fim %></dd>
  <dt><strong><%= model_class.human_attribute_name(:prazoboleto) %>:</strong></dt>
  <dd><%= @vestibulare.prazoboleto %></dd>
  <dt><strong><%= model_class.human_attribute_name(:prova) %>:</strong></dt>
  <dd><%= @vestibulare.prova %></dd>
  <dt><strong><%= model_class.human_attribute_name(:status) %>:</strong></dt>
  <dd><%= @vestibulare.status %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              vestibulares_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_vestibulare_path(@vestibulare), :class => 'btn btn-primary' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              vestibulare_path(@vestibulare),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
